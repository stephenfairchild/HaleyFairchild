{
    "family": "haleyfairchild",
    "networkMode": "bridge",
    "volumes": [
        {
            "name": "app",
            "host": {}
        }
    ],
    "containerDefinitions": [
        {
            "name": "haleyfairchild",
            "image": "046470573167.dkr.ecr.us-east-2.amazonaws.com/haleyfairchild.com:latest",
            "cpu": 450,
            "memory": 600,
            "memoryReservation": 20,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 80
                }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "app",
                    "containerPath": "/var/www/html"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "MYSQL_DATABASE",
                    "value": "haleyfairchild_main"
                },
                {
                    "name": "APPLICATION_ENV",
                    "value": "production"
                }
            ]
        }
    ]
}
